//scr_label1 := s40 ? scr_label1 + syminfo.ticker(a40) + '\n' : scr_label1
lab_2 = label.new(bar_index+15 + loc,close, scr_label1, color=color.red, textcolor=color.white, style=label.style_label_center, force_overlay=true)
label.delete(lab_2[1])
if str.length(scr_label1) > 8
    alert(scr_label1,alert.freq_once_per_bar_close)



// EMA gösterim ayarları
showEMA8 = input.bool(false, "EMA 8 Göster")
showEMA21 = input.bool(false, "EMA 21 Göster")
showEMA34 = input.bool(true, "EMA 34 Göster")   // Başlangıçta açık
showEMA55 = input.bool(false, "EMA 55 Göster")
showEMA100 = input.bool(false, "EMA 100 Göster")
showEMA200 = input.bool(false, "EMA 200 Göster")

// EMA hesaplamaları
ema8 = ta.ema(close, 8)
ema21 = ta.ema(close, 21)
ema34 = ta.ema(close, 34)
ema55 = ta.ema(close, 55)
ema100 = ta.ema(close, 100)
ema200 = ta.ema(close, 200)

// EMA'ları çiz, görünürlük na ile kontrol edilir
plot(showEMA8 ? ema8 : na, color=color.orange, title="EMA 8", force_overlay=true)
plot(showEMA21 ? ema21 : na, color=color.yellow, title="EMA 21", force_overlay=true)
plot(showEMA34 ? ema34 : na, color=color.blue, title="EMA 34", force_overlay=true)
plot(showEMA55 ? ema55 : na, color=color.purple, title="EMA 55", force_overlay=true)
plot(showEMA100 ? ema100 : na, color=color.gray, title="EMA 100", force_overlay=true)
plot(showEMA200 ? ema200 : na, color=color.red, title="EMA 200", force_overlay=true)
