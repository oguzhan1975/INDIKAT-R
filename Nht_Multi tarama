rsiValue     = ta.rsi(close, 14)
momentum     = ta.mom(close, 10)
priceChange  = (close - close[1]) / close[1] * 100
volumeCond   = volume
//ema50        = ta.ema(close, 50)
rvol         = volume / ta.sma(volume, 20)
[macdLine, signalLine, macdHist] = ta.macd(close, 12, 26, 9)
//[plusDI, minusDI, adx] = ta.dmi(14, 14)

kosul1w = rsiValue > 70
kosul2w = momentum > 100
kosul3w = priceChange > 0
kosul4w = volumeCond > 50000
kosul5w = close > ema50
kosul6w = adx > 20
kosul7w = rvol > 1.5
kosul8w = macdHist > 0
alKosuluw = kosul1w and kosul2w and kosul3w and kosul4w and kosul5w and kosul6w and kosul7w and kosul8w
