//@version=5
indicator("Nht_Multi tarama", overlay=true)
// Girdi parametreleri tren başlangıcı
rsiShortLen = 5
rsiLongLen  = 14
emaLen      = 8
adxLen      = 14
momLen      = 10
hacimUzun   = 10

// RSI ve EMA
rsiShort = ta.rsi(close, rsiShortLen)
rsiLong  = ta.rsi(close, rsiLongLen)
ema8     = ta.ema(close, emaLen)
mom      = ta.mom(close, momLen)

// Ortalama hacim ve bağıl hacim
hacimOrt = ta.sma(volume, hacimUzun)
bagilHacim = volume / hacimOrt

// 1 haftalık performans
perf1w = (close - close[5]) / close[5] * 100

// Hacim değişimi
hacimDegisim = (volume - volume[1]) / volume[1] * 100

// ADX Hesaplama (manuel)
up = ta.change(high)
down = -ta.change(low)
plusDM = (up > down and up > 0) ? up : 0
minusDM = (down > up and down > 0) ? down : 0
trur = ta.rma(ta.tr(true), adxLen)
plusDI = 100 * ta.rma(plusDM, adxLen) / trur
minusDI = 100 * ta.rma(minusDM, adxLen) / trur
dx = 100 * math.abs(plusDI - minusDI) / (plusDI + minusDI)
adx = ta.rma(dx, adxLen)
// Koşullar
kosul1 = perf1w < 15
kosul2 = ta.crossover(rsiShort, rsiLong)
kosul3 = ta.crossunder(ema8, close)
kosul4 = hacimDegisim > 30
kosul5 = adx > 14
kosul6 = bagilHacim > 1
kosul7 = mom > -10
kosul8 = volume > hacimOrt
alKosulu = kosul1 and kosul2 and kosul3 and kosul4 and kosul5 and kosul6 and kosul7 and kosul8
sat_sinyali = ta.crossunder(rsiLong, 70)  
// hacim patlaması

// Girişler
price = close
perf1w_threshold = 10.0  // 1 haftalık performans sınırı
degisim_min = 0.0
degisim_max = 9.5
hacimDegisimMin = 100.0
bagilHacimMin = 3.0

// Hesaplamalar
degisimYuzde = (close - close[1]) / close[1] * 100
//perf1w = (close - close[5]) / close[5] * 100
//hacimDegisim = (volume - volume[1]) / volume[1] * 100
bagilHacimm = volume / ta.sma(volume, 20)

// Koşullar
kosul1a = degisimYuzde >= degisim_min and degisimYuzde <= degisim_max
kosul2a = perf1w < perf1w_threshold
kosul3a = hacimDegisim > hacimDegisimMin
kosul4a = bagilHacimm > bagilHacimMin

// Tüm koşullar sağlanırsa AL sinyali
alSinyali = kosul1a and kosul2a and kosul3a and kosul4a
////////////////////////////////////////////////////////
//RD KAPLUMBAĞA
// === Göstergeler ===
sma20 = ta.sma(close, 20)
atr14 = ta.atr(14)

// === Stokastik RSI (3,3,14,14) ===
rsi = ta.rsi(close, 14)
stochRsi = (rsi - ta.lowest(rsi, 14)) / (ta.highest(rsi, 14) - ta.lowest(rsi, 14))
k = ta.sma(stochRsi, 3)
d = ta.sma(k, 3)

// === Performans ve Hacim Koşulları ===
haftalik_degis = (close - close[5]) / close[5] * 100
bag_hacim = volume / ta.sma(volume, 20)

// === Koşullar ===
kosul1b = haftalik_degis < 10
kosul2b = bag_hacim > 1
kosul3b = atr14 < 1
kosul4b = ta.crossover(close, sma20)
kosul5b = k >= d
al_sinyali = kosul1b and kosul2b and kosul3b and kosul4b and kosul5b
///////////////////////
//Dahi
// === Fiyat ve Hacim ===
fiyat = close
degisim1w = (close - close[5]) / close[5] * 100       // 5 günlük değişim ~ 1 hafta
degisim = (close - close[1]) / close[1] * 100          // 1 günlük değişim
hacimOran = volume / ta.sma(volume, 20)

// === DMI (14,14) ===
//[plusDI, minusDI, adx] = ta.dmi(14, 14)

// === Aroon (14) === - Manuel hesaplama
aroonLength = 14
highestIndex = ta.barssince(high == ta.highest(high, aroonLength))
lowestIndex  = ta.barssince(low == ta.lowest(low, aroonLength))
aroonUp   = 100 * (aroonLength - highestIndex) / aroonLength
aroonDown = 100 * (aroonLength - lowestIndex)  / aroonLength

// === Hull HO (9) ===
hull = ta.wma(2 * ta.wma(close, 9 / 2) - ta.wma(close, 9), math.round(math.sqrt(9)))

// === Ichimoku (9,26,52,26) ===
conversionLine = (ta.highest(high, 9) + ta.lowest(low, 9)) / 2
baseLine = (ta.highest(high, 26) + ta.lowest(low, 26)) / 2

// === KOŞULLAR ===
kosul1c = degisim1w < 10                     // 1 Haftalık değişim < 10%
kosul2c = plusDI > minusDI                   // DMI pozitif > negatif
kosul3c = aroonUp == 100                     // Aroon Up %100
kosul4c = hacimOran > 1.7                    // Hacim ortalamanın 1.7 katı
kosul5c = aroonUp > aroonDown               // Aroon Up > Down
kosul6c = adx > 20                           // ADX > 20
kosul7c = hull < fiyat                       // Hull < Fiyat
kosul8c = baseLine < conversionLine         // Ichimoku Base < Conversion
kosul9c = degisim < 9.5                      // Günlük değişim < 9.5%

// === TÜM KOŞULLAR SAĞLANIYORSA ===
var table myTable = table.new(position.bottom_right, 1, 1, border_width=1, frame_color=color.black, bgcolor=color.white)
al_sinyalii = kosul1c and kosul2c and kosul3c and kosul4c and kosul5c and kosul6c and kosul7c and kosul8c and kosul9c
/////////
//ZKN en iyi alım yeri
// === Göstergeler ===
sma50 = ta.sma(close, 50)

// Fibonacci Pivot (Günlük)
fibo_p = (ta.highest(high, 1) + ta.lowest(low, 1) + close[1]) / 3

// Klasik Pivot (Günlük)
klasik_p = (high + low + close) / 3

// Değişim (1W ve Günlük)
change1w = 100 * (close - close[5]) / close[5]
changeToday = 100 * (close - close[1]) / close[1]

// Bağıl Hacim
relativeVolume = volume / ta.sma(volume, 20)

// === Koşullar ===
cond1 = ta.crossover(close, sma50)
cond2 = ta.crossover(close, fibo_p)
cond3 = ta.crossover(close, klasik_p)
cond4 = change1w < 15
cond5 = changeToday < 9.5
cond6 = relativeVolume > 1

// === Tüm koşullar sağlanırsa AL sinyali ===
allConditions = cond1 and cond2 and cond3 and cond4 and cond5 and cond6
/////////////////////////
// trend başlangıcı
// === Göstergeler ===
bb_basis   = ta.sma(close, 20)
ema50      = ta.ema(close, 50)
volume_avg = ta.sma(volume, 20)
//rsi        = ta.rsi(close, 14)
// === AL Koşulları ===
bb_kesme   = ta.crossover(close, bb_basis)                     // Fiyat BB basis yukarı keser
ema_kesme  = ta.crossover(close, ema50)                        // Fiyat EMA50 yukarı keser
perf_1w    = (close - close[5]) / close[5] * 100               // 1 haftalık performans
//degisim    = math.abs(close - close[1]) / close[1] * 100       // Günlük yüzde değişim
bag_hacimm  = volume / volume_avg                               // Bağıl 
table.cell(myTable, 0, 0, "Join Telegram @Combo Code", bgcolor=color.blue, text_color=color.white, text_size=size.small)
al_sinyali1 = bb_kesme and ema_kesme and perf_1w < 10 and degisim < 9.5 and bag_hacimm > 1
// === SAT Koşulu ===
////////////////////////////////////////////////////////////////////
//Dahi2

// === Göstergeler ===
//change1w = (close - close[5]) / close[5] * 100
volumeRatio = volume / ta.sma(volume, 20)
[plusDI1, minusDI1, adx1] = ta.dmi(14, 14)
sma200 = ta.sma(close, 200)
//sma50 = ta.sma(close, 50)

// Aroon (manuel)
len = 14
aroonUpp = 100 * (len - ta.barssince(high == ta.highest(high, len))) / len
aroonDownn = 100 * (len - ta.barssince(low == ta.lowest(low, len))) / len

// SAR (tüm parametreler verildi)
sar = ta.sar(0.02, 0.02, 0.2)

// Günlük değişim
changeNow = (close - close[1]) / close[1] * 100

// CMF (manuel - ta.sum yerine güvenli yöntem)
cmfLen = 20
clv = ((close - low) - (high - close)) / math.max(high - low, 0.00001)
cmf = ta.ema(clv * volume, cmfLen) / ta.ema(volume, cmfLen)

// Hull MA
hull1 = ta.wma(2 * ta.wma(close, 9 / 2) - ta.wma(close, 9), math.round(math.sqrt(9)))

// Ichimoku
conversionLinee = (ta.highest(high, 9) + ta.lowest(low, 9)) / 2
baseLinee = (ta.highest(high, 26) + ta.lowest(low, 26)) / 2

// === Koşullar ===
kosul1b1  = change1w < 10
kosul2b1  = volumeRatio > 1
kosul3b1  = adx1 > 20
kosul4b1  = plusDI1 > minusDI1
kosul5b1  = sma200 < sma50
kosul6b  = aroonUpp == 100
kosul7b  = aroonDownn < aroonUpp
kosul8b  = sar < close
kosul9b  = changeNow < 9.5
kosul10b = cmf > 0
kosul11b = hull1 < close
kosul12b = baseLinee < conversionLinee

// === AL Sinyali ===
alKosulu2 = kosul1b1 and kosul2b1 and kosul3b1 and kosul4b1 and kosul5b1 and kosul6b and kosul7b and kosul8b and kosul9b and kosul10b and kosul11b and kosul12b
////////////////////////////////////////////
