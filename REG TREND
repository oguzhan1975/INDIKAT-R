    cond = longCondition
    // cond = ta.crossover(close[1], rma[1]) or (close[1] >= rsi0[1] and close[1] <= rsi10[1]) or ta.crossover(close, rsi10[1]) or ta.crossover(f_var(kaynak, uzunluk), f_var(kaynak, uzunluk1)) or ta.crossover(f_var(kaynak, uzunluk), rma) 

    [close, cond]



